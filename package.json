{
  "name": "react-read-pdf",
  "version": "2.0.8",
  "description": "a mobile-friendly  PDF Reader in browser for React",
  "keywords": [
    "react",
    "pdf",
    "pdfjs",
    "react-read-pdf"
  ],
  "main": "lib/app.js",
  "author": "yuxl",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/forthealllight/react-read-pdf.git"
  },
  "bugs": {
    "url": "https://github.com/forthealllight/react-read-pdf/issues"
  },
  "homepage": "https://github.com/forthealllight/react-read-pdf",
  "scripts": {
    "build": "yarn run clean-dist && webpack -p --config=configs/webpack/prod.js",
    "clean-dist": "rm -f -r -d dist",
    "clean-lib": "rm -f -r -d lib",
    "lint": "yarn run lint:ts && yarn run lint:sass",
    "lint:ts": "tslint --fix './src/**/*.ts*' --format stylish --force",
    "lint:sass": "stylelint ./src/**/*.scss",
    "start": "yarn run start-dev",
    "start-dev": "webpack-dev-server --config=configs/webpack/dev.js --open --inline",
    "start-prod": "yarn run build && node express.js",
    "test": "jest --watch --coverage --config=configs/jest.json",
    "lib": "yarn run clean-lib && webpack --config=configs/webpack/lib.js  --progress --colors"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@types/jest": "^23.3.2",
    "@types/node": "^10.10.2",
    "@types/react": "^16.4.14",
    "@types/react-dom": "^16.0.7",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.2",
    "css-loader": "^1.0.0",
    "express": "^4.16.3",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.3.1",
    "jest": "^23.6.0",
    "node-sass": "^4.9.3",
    "react": "^16.5.2",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.5.2",
    "react-hot-loader": "^4.3.11",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "stylelint": "^9.5.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "tslint": "^5.11.0",
    "typescript": "^3.0.3",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.19.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-middleware": "^3.3.0",
    "webpack-dev-server": "^3.1.8",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-preset-stage-2": "^6.24.1",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "pdfjs-dist": "^2.0.550",
    "react-css-modules": "^4.7.7",
    "ts-loader": "^5.2.2",
    "typings-for-css-modules-loader": "^1.7.0",
    "url-loader": "^1.1.2"
  }
}
